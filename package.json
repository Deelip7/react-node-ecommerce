{
  "name": "react-node-ecommerce",
  "version": "1.0.0",
  "type": "module",
  "description": "Rn Store",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destory": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deelip7/react-node-ecommerce.git"
  },
  "author": "Theelipan Prabakar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deelip7/react-node-ecommerce/issues"
  },
  "homepage": "https://github.com/Deelip7/react-node-ecommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
